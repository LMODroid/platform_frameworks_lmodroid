android_app {
    name: "LMOManifest",
    certificate: "platform",
    platform_apis: true,
    system_ext_specific: true,

    // Disable dexpreopt and verify_uses_libraries check as the app
    // contains no Java code to be dexpreopted.
    enforce_uses_libs: false,
    dex_preopt: {
        enabled: false,
    },
}
